public virtual void AssignGuest(Guest guest)
    {
        if (!IsOccupied)
        {
            Occupant = guest;
            IsOccupied = true;
            Console.WriteLine($"Guest {guest.Name} har blivit tilldelad till rum {RoomNumber}.");
        }
        else
        {
            Console.WriteLine($"Room {RoomNumber} Ã¤r redan upptaget.");
        }
    }
