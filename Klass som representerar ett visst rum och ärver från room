public class SingleRoom : Room
{
    public SingleRoom(int roomNumber)
    {
        RoomNumber = roomNumber;
    }
}

public class DoubleRoom : Room
{
    public DoubleRoom(int roomNumber)
    {
        RoomNumber = roomNumber;
    }
}

public class Suite : Room
{
    public Suite(int roomNumber)
    {
        RoomNumber = roomNumber;
    }
}
