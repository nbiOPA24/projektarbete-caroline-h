using System;

public class Guest
{
    public string Name { get; set; }
    public int Age { get; set; }

    public Guest(string name, int age)
    {
        Name = name;
        Age = age;
    }

    public void DisplayInfo()
    {
        Console.WriteLine($"Namn: {Name}, Ã…lder: {Age}");
    }
}
